---
const { salmon } = Astro.props;
---
{salmon != "chinook" && (
    <div class="bg-white p-4 m-4 shadow text-blue-700 font-semibold">
        <a href="/salmon/chinook/">Chinook (King) salmon fishing near Medford, Oregon</a>
    </div>
    )
}
{salmon != "coho" && (
    <div class="bg-white p-4 m-4 shadow text-blue-700 font-semibold">
        <a href="/salmon/coho/">Coho (Silver) salmon fishing near Medford, Oregon</a>
    </div>
    )
}
